            <stringProp name="script">import java.util.Random;
import java.text.SimpleDateFormat;
import java.util.Date;


// 随机生成其他字段
Random random = new Random();
double randomAmount = random.nextDouble() * 5000;
int accountId = random.nextInt(1000);
int randomType = random.nextInt(1);
String randomDescription = &quot;Random description &quot; + random.nextInt(100);
SimpleDateFormat sdf = new SimpleDateFormat(&quot;yyyy-MM-dd HH:mm:ss&quot;);
String randomDateTime = sdf.format(new Date());

// 保存到变量中
vars.put(&quot;amount&quot;, Double.toString(randomAmount));
vars.put(&quot;accountId&quot;, &quot;AC&quot; + accountId);
vars.put(&quot;type&quot;, Integer.toString(randomType));
vars.put(&quot;description&quot;, randomDescription);
vars.put(&quot;dateTime&quot;, randomDateTime);

log.info(&quot;Current ID: &quot; + currentId + &quot;, Current BizID: &quot; + currentBizId);
log.info(&quot;Amount: &quot; + randomAmount);
log.info(&quot;Type: &quot; + randomType);
log.info(&quot;Description: &quot; + randomDescription);
log.info(&quot;Date Time: &quot; + randomDateTime);</stringProp>
